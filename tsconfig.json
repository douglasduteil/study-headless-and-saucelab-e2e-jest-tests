{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["dom", "es2015"],
    "module": "es2015",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2017",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/jest-environment-puppeteer/@types"
    ]
  }
}
